<div class="content">
	<div style="padding: 10px;">
	<% form_for :gallery, @gallery, :url => {:action => "create"} do |f| %>
		<div class="field">
			<label for="name">Name</label>
			<%= f.text_field :title %>
		</div>
		<div class="field">
			<label for="phrase">Description</label>
			<%= f.text_area :description %>
		</div>
		<div class="submit">
			<%= f.submit "Create"%>
		<% end %>
		</div>
	</div>